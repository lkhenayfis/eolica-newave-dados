{
    // Nome do estudo, sera usado para gerar uma pasta em out/estima_ftm com os resultados
    "tag": "DEFAULT",
    
    // Booleano indicando se o diretorio deve ser limpo antes de salvar novos resultados
    "limpadir": false,

    // Vetor de subsistemas para rodar
    "Subsistemas": ["NE", "S"],

    // Lista de modelos de compactacao. Cada elemento deve ser uma lista contendo, na primeira 
    // posicao, o nome do metodo (um dos implementados em clustcens) e os demais os argumentos
    // da funcao de compactacao. O dado a ser compactado nao precisa ser informado aqui, pois 
    // sera automaticamente definido dentro do script
    "mod_compact": {
        "PCA_07": {"fun": "PCA", "vartot": 0.7},
        "PCA_08": {"fun": "PCA", "vartot": 0.8},
        "PCA_09": {"fun": "PCA", "vartot": 0.9}
    },

    // Lista de modelos de clusterizacao. Cada elemento deve ser uma lista contendo, na primeira 
    // posicao, o nome do metodo (um dos implementados em clustcens) e os demais os argumentos
    // da funcao de clusterizacao. O dado a ser clusterizado nao precisa ser informado aqui, pois 
    // sera automaticamente definido dentro do script
    "mod_cluster": {
        "EM_2": {"fun": "EM", "n": 2},
        "EM_3": {"fun": "EM", "n": 3},
        "EM_4": {"fun": "EM", "n": 4},
        "EM_5": {"fun": "EM", "n": 5},
        "kmeans_2": {"fun": "kmeans", "n": 2, "nstart": 200},
        "kmeans_3": {"fun": "kmeans", "n": 3, "nstart": 200},
        "kmeans_4": {"fun": "kmeans", "n": 4, "nstart": 200},
        "kmeans_5": {"fun": "kmeans", "n": 5, "nstart": 200}
    }
}